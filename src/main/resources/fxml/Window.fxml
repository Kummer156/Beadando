<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="469.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="beadando.controllers.WindowController">
    <children>
        <TabPane layoutX="14.0" layoutY="14.0" prefHeight="444.0" prefWidth="473.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab text="Pizza">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <TableView fx:id="table1" prefHeight="415.0" prefWidth="473.0">
                                </TableView>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab fx:id="tab2" onSelectionChanged="#refresh" text="Cart">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <TableView fx:id="table2">
                                </TableView>
                        <Button fx:id="orderbtn" layoutX="381.0" layoutY="376.0" mnemonicParsing="false" onAction="#PlaceOrder" text="Place Order" />
                        <Button fx:id="logoutbtn" layoutX="405.0" layoutY="14.0" mnemonicParsing="false" onAction="#Logout" text="Logout" />
                        <Label fx:id="total" layoutX="381.0" layoutY="349.0" text="Label" />
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
            <Tab text="User Information">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextArea fx:id="userInformation" editable="false" layoutX="14.0" layoutY="14.0" prefHeight="386.0" prefWidth="443.0" wrapText="true" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
